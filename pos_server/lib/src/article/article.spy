class: Article
table: article
fields:
  id: UuidValue, defaultPersist=random, defaultModel=random
  name: String,
  description: String?
  price: double,
  categorie: Categorie?, relation(onDelete=setNull)
  composition: List<ArticleComposition>?, relation(optional)

indexes:
  article_categorieId_idx:
    fields: categorieId