class: Access
table: access
fields:
  id: UuidValue, defaultPersist=random, defaultModel=random
  name: String
  consultAllOrders: bool,  ### false mean consult only his passed orders
  orderCreation: bool
  orderCreationNotif: bool,
  orderPayment: bool
  orderItemsPayment: bool
  appendItems: bool,
  preparation: bool,
  serveOrder: bool,
  caisseManagement: bool
  buildingId: UuidValue, relation(parent=building)


indexes:
  access_buildingId_idx:
    fields: buildingId