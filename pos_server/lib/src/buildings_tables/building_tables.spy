class: BTable
table: b_tables
fields:
  id: UuidValue, defaultPersist=random, defaultModel=random
  ### The number of the building.
  number: int
  seatsMax: int
  status: TableStatus?,default=available, !persist ### This field is not persisted in the database.
  buildingId: UuidValue, relation(parent=building)
  active: bool, default=true
indexes:
  btables_buildingId_idx:
    fields: buildingId